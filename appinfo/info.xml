<?xml version="1.0"?>
<info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>helplinks</id>
    <name>Help Links Manager</name>
    <summary>Manage hierarchical help links for your Nextcloud instance</summary>
    <description><![CDATA[Create and manage customizable help sections with main topics and sub-links accessible through the admin settings.]]></description>
    <version>1.0.3</version>
    <licence>agpl</licence>
    <author>Tom Wezepoel</author>
    <namespace>HelpLinks</namespace>
    <category>SURF</category>
    <bugs>https://github.com/sara-nl/nextcloud-helplinks-app/issues</bugs>
    <dependencies>
        <nextcloud min-version="30" max-version="32"/>
    </dependencies>
    <navigations>
        <navigation>
            <name>Help</name>
            <route>helplinks.page.index</route>
            <icon>help.png</icon>
        </navigation>
    </navigations>
    <settings>
        <admin>OCA\HelpLinks\Settings\AdminSettings</admin>
    </settings>
</info>